{
  "name": "reflective-coffeescript",
  "description": "Reflection-augmented CoffeeScript",
  "keywords": [
    "reflection",
    "language",
    "coffeescript",
    "compiler"
  ],
  "author": "Michele Bini, Jeremy Ashkenas",
  "version": "1.8.0-reflective.100",
  "license": "MIT",
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "lib": "./lib/coffee-script"
  },
  "main": "./lib/coffee-script/coffee-script",
  "bin": {
    "coffee": "./bin/coffee",
    "cake": "./bin/cake"
  },
  "preferGlobal": true,
  "scripts": {
