    "test-harmony": "node --harmony ./bin/cake test"
  },
  "homepage": "http://rev22.github.io/reflective-coffeescript",
  "bugs": "https://github.com/rev22/reflective-coffeescript/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/rev22/reflective-coffeescript.git"
  },
  "devDependencies": {
    "uglify-js": "~2.2",
    "jison": ">=0.2.0",
    "highlight.js": "~8.0.0",
    "underscore": "~1.5.2",
    "docco": "~0.6.2"
  },
  "dependencies": {
    "mkdirp": "~0.3.5",
    "highlight.js": "~8.0"
  }
}
